  particles:
    '1':
      type: PORTAL
      range-x: 2.0
      range-y: 2.0
      range-z: 2.0
      speed: 0.0
      amount: 20
      animation: NONE
      center-x: 0
      center-y: 0
      center-z: 0
    '2':
      type: FLAME
      range-x: 0.0
      range-y: 0.0
      range-z: 0.0
