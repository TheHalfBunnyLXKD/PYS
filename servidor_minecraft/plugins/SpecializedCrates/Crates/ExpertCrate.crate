      center-x: 0
      center-y: 0
      center-z: 0

open:
  crate-animation: INV_ENCLOSE
  
  particles:
    '1':
      type: SUSPENDED_DEPTH
      range-x: 3.0
      range-y: 2.0
      range-z: 3.0
      speed: 1.0
      amount: 30
      animation: NONE
      center-x: 0
      center-y: 0
      center-z: 0
    '2':
      type: CRIT_MAGIC
      range-x: 3.0
      range-y: 2.0
      range-z: 3.0
      speed: 1.0
      amount: 10
      animation: NONE
      center-x: 0
      center-y: 0
      center-z: 0
